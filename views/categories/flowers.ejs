<%- include("../partials/header")%>
<div class="category">
<head>
    <!-- Meta Tags -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Farmgate Garden & Floriculture Services – Flowers, Landscaping & More</title>
    <meta name="description" content="Farmgate Nigeria connects farmers and florists with customers nationwide. We offer landscaping, floriculture services, and sell natural & artificial flowers.">
    <meta name="keywords" content="farmgate Nigeria, floriculture services, landscaping, flower shop Nigeria, natural flowers, artificial flowers, garden maintenance, ornamental flowers">
    <meta name="robots" content="index, follow">

    <!-- Open Graph / Facebook -->
    <meta property="og:title" content="Farmgate Garden & Floriculture Services – Flowers & Landscaping">
    <meta property="og:description" content="Showcase your flowers and gardening expertise. Farmgate Nigeria helps florists connect with customers nationwide and sell flowers and landscaping services.">
    <meta property="og:type" content="website">
    <meta property="og:image" content="<%= image %>">
    <meta property="og:url" content="https://farmgate.com.ng/flowers">

    <!-- Twitter Card -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="Farmgate Garden & Floriculture Services">
    <meta name="twitter:description" content="Join Farmgate Nigeria to showcase flowers, artificial plants, and landscaping services. Connect with clients nationwide.">
    <meta property="og:image" content="<%= image %>">

    <!-- Canonical URL -->
    <link rel="canonical" href="<%= canonicalUrl %>">
    <!-- JSON-LD Schema -->
    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "Organization",
      "name": "Farmgate Nigeria",
      "url": "https://farmgate.com.ng",
      "logo": "https://farmgate.com.ng/pics/logo.png",
      "sameAs": [
        "https://www.facebook.com/Koyegarden",
        "https://www.instagram.com/farmgatngr"
      ],
      "description": "Farmgate Nigeria connects farmers and florists with customers nationwide. We offer garden and floriculture services, including landscaping, flower arrangement, pest control, and sales of natural and artificial flowers.",
      "department": {
        "@type": "Organization",
        "name": "Garden & Floriculture Services",
        "url": "https://farmgate.com.ng/flowers",
        "description": "Providing landscaping, garden maintenance, natural and artificial flowers for homes, offices, and events."
      }
    }
    </script>

    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "Service",
      "name": "Garden & Floriculture Services",
      "description": "Expert landscaping, flower arrangements, maintenance of indoor and outdoor plants, and pest-control services across Nigeria.",
      "provider": {
        "@type": "Organization",
        "name": "Farmgate Nigeria",
        "url": "https://farmgate.com.ng"
      },
      "areaServed": {
        "@type": "Country",
        "name": "Nigeria"
      }
    }
    </script>

    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "FAQPage",
      "mainEntity": [
        {
          "@type": "Question",
          "name": "What do we specialise in doing?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Planting and propagation of flowers; Maintenance and landscaping of flower gardens; Sales of natural and artificial flowers; Fumigation and pest-control services."
          }
        },
        {
          "@type": "Question",
          "name": "How quick do you handle delivery?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "All deliveries within South-west region (Lagos, Oyo, Osun, Ogun, Ondo, Ekiti) occur within 1 to 3 days."
          }
        },
        {
          "@type": "Question",
          "name": "Will I need to pay for flowers before delivery?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "No, you do not need to make payment before delivery, but a maximum of 2000 Naira may be required for delivery logistics."
          }
        }
      ]
    }
    </script>
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "ItemList",
  "name": "Available Flowers for Sale",
  "description": "A list of natural and artificial flowers available for sale on Farmgate Nigeria.",
  "url": "https://farmgate.com.ng/flowers",
  "itemListElement": [
    <% seedlings.forEach(function(crops, index){ %>
    {
      "@type": "ListItem",
      "position": <%= index + 1 %>,
      "item": {
        "@type": "Product",
        "name": "<%= crops.name %>",
        "image": "<%= crops.image[0].url %>",
        "description": "<%= crops.description %>",
        "url": "https://farmgate.com.ng/<%= crops._id %>",
        "offers": {
          "@type": "Offer",
          "price": "<%= crops.price || 0 %>",
          "priceCurrency": "NGN",
          "availability": "https://schema.org/InStock"

        },
         "seller": {
        "@type": "Person"
      }
      }
    }<% if (index !== seedlings.length - 1) { %>,<% } %>
    <% }) %>
  ]
}
</script>

</head>

    
